<table id="medication-table">
  <header>
    <th>Name</th>
    <th>Description</th>
    <th>Quantity Remaining</th>
    <th>Dose</th>
  </header>
  <% @medications.each do |med| %>
    <tr>
      <td><%= med.name %></td>
      <td><%= med.description %></td>
      <td><%= med.remaining %></td>
    </tr>
  <% end %>
</table>